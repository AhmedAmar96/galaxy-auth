{
  "name": "auth-microservice",
  "version": "0.0.1",
  "description": "",
  "author": "dafholding",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "db:diagram": "ts-node ./node_modules/.bin/typeorm-uml ormconfig.uml.json",
    "start:devv": "cross-env NODE_ENV=development nodemon --config nodemon.json",
    "start:dev": "set NODE_ENV=development && npx kill-port 1000 && nest start --watch",
    "start:dev:db": "cross-env NODE_ENV=development ./src/Core/scripts/start-db.sh",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "rimraf": "./node_modules/rimraf/bin.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test jest --watch --detectOpenHandles",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json",
    "pretypeorm": "(del /f ormconfig.json || :) && ts-node -r tsconfig-paths/register src/Core/scripts/write-type-orm-config.ts",
    "typeorm": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:generate": "npm run typeorm -- migration:generate  -n",
    "typeorm:migration:run": "npm run typeorm -- migration:run ",
    "migration:revert": "ts-node node_modules/.bin/typeorm migration:revert ",
    "start:dev:db:seed": "ts-node -r tsconfig-paths/register src/Core/scripts/seed.ts"
  },
